{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": true
				}
			},
			"core/image": {
				"appearanceTools": true,
				"border": {
					"color": false,
					"style": false,
					"width": false
				},
				"color": {
					"background": true,
					"custom": true,
					"customDuotone": true,
					"customGradient": true,
					"defaultDuotone": true,
					"defaultGradients": true,
					"defaultPalette": true,
					"duotone": [],
					"gradients": [],
					"link": false,
					"palette": [],
					"text": true
				},
				"lightbox": {
					"allowEditing": true,
					"enabled": true
				},
				"shadow": {
					"defaultPresets": true,
					"presets": [
						{
							"name": "Laks A",
							"shadow": "2px 2px 15px rgba(0, 0, 0, 0.2)",
							"slug": "laks a"
						}
					]
				},
				"spacing": {
					"customSpacingSize": true,
					"defaultSpacingSizes": true,
					"spacingScale": {
						"default": {
							"increment": 1.5,
							"mediumStep": 1.5,
							"operator": "*",
							"steps": 7,
							"unit": "rem"
						}
					},
					"spacingSizes": [],
					"units": false
				},
				"typography": {
					"customFontSize": true,
					"defaultFontSizes": true,
					"dropCap": true,
					"fontFamilies": [
						{
							"fontFamily": "\"Yu Gothic\", sans-serif",
							"name": "Yu Gothic",
							"slug": "yu-gothic"
						}
					],
					"fontSizes": [],
					"fontStyle": true,
					"fontWeight": true,
					"letterSpacing": true,
					"lineHeight": false,
					"textAlign": true,
					"textDecoration": true,
					"textTransform": true,
					"writingMode": false
				},
				"useRootPaddingAwareAlignments": false
			}
		},
		"dimensions": {
			"minHeight": true
		},
		"shadow": {
			"defaultPresets": true,
			"presets": [
				{
					"name": "Laks A",
					"shadow": "2px 2px 15px rgba(0, 0, 0, 0.2)",
					"slug": "laks a"
				}
			]
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": []
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"css": ""
			},
			"core/group": {
				"css": ""
			},
			"core/home-link": {
				"css": "margin-right: 1.2rem;\nmargin-left: 1.2rem;"
			},
			"core/image": {
				"css": "",
				"variations": {
					"rounded": {
						"css": ""
					}
				}
			},
			"core/media-text": {
				"css": "border-radius: 8px;\noverflow: hidden;"
			},
			"core/navigation-link": {
				"css": "margin-right: 1.2rem;\nmargin-left: 1.2rem;"
			},
			"core/post-featured-image": {
				"css": ""
			},
			"core/post-template": {
				"css": ""
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--pale-pink)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--pale-pink)"
						}
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/query-pagination-next": {
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/query-pagination-numbers": {
				"css": "letter-spacing: 0.7em"
			},
			"core/query-pagination-previous": {
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/separator": {
				"color": {
					"background": "#909090c2"
				}
			}
		},
		"color": {
			"background": "#f7f7f8",
			"text": "var(--wp--preset--color--black)"
		},
		"css": ".is-style-glass.left{max-width: 800px;}\n.is-style-glass.right{max-width: 400px;}\n@media screen and (max-width: 800px){.is-style-glass.right{max-width: 800px;}}",
		"elements": {
			"button": {
				"color": {
					"background": "#ffd4d4",
					"text": "var(--wp--preset--color--pale-pink)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--yu-gothic)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--yu-gothic)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--pale-pink)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--yu-gothic)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--yu-gothic)"
				}
			}
		},
		"spacing": [],
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--yu-gothic)"
		}
	},
	"version": 3
}