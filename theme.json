{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"dimensions": {
			"minHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans JP",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/noto-sans-jp.ttf"
						}
					],
					"fontFamily": "\"Noto Sans JP\", sans-serif",
					"name": "Noto Sans JP",
					"slug": "body"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "#18181e",
			"text": "#8ff2b0"
		},
		"css": ".navigation {\nletter-spacing: 6px;\n}\n    ",
		"elements": {
			"caption": {
				"color": {
					"text": "#1fbe84"
				}
			},
			"heading": {
				"color": {
					"text": "#83d6f0"
				}
			},
			"link": {
				"color": {
					"text": "#8ff2b0"
				}
			}
		}
	},
	"version": 3
}