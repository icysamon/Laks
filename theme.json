{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"dimensions": {
			"minHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans JP",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/noto-sans-jp.ttf"
						}
					],
					"fontFamily": "\"Noto Sans JP\", sans-serif",
					"name": "Noto Sans JP",
					"slug": "body"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "#fefefe",
			"text": "var(--wp--preset--color--black)"
		},
		"css": "",
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--pale-pink)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--vivid-cyan-blue)"
				}
			},
			"heading": {
				"color": {
					"text": "#ef9b9b"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			}
		}
	},
	"version": 3
}